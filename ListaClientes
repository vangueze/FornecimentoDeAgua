public class ListaClientes extends Cadastro{

  Cadastro cadastro = new Cadastro();

  public void visualizarClientes(){
    System.out.println("*************************************");
    System.out.println("Nome : "+cadastro.nome+"\nApelido : "+cadastro.apelido+"\nBairro : "+cadastro.bairro+"
    +"\nRua_Avenida : "+cadastro.rua_avenida+"\nCidade : "+cadastro.cidade+"\nContrato : "+cadastro.contrato);
    System.out.println("*************************************");

  }
}
